{{#modal-dialog 
  targetAttachment="center"
  close=(action transitionToRoute "warehouse.admin.index") 
  translucentOverlay=true}}
  {{#modal-view as |yields|}}
    {{#yields.header}}
      <h6>Tiles Index - {{model.length}}<small>{{portal-for name="header-subtext"}}</small></h6>
    {{/yields.header}}
    {{#yields.links}}
      <ul class="nav nav-tabs row">
        {{#nav-tab "warehouse.admin.cells.index"}}
          {{fa-icon "cubes" fixedWidth=true}} Storage Cells
        {{/nav-tab}}
        {{#nav-tab "warehouse.admin.desks.index"}}
          {{fa-icon "desktop" fixedWidth=true}} Logistic Desks
        {{/nav-tab}}
        {{#nav-tab "warehouse.admin.docks.index"}}
          {{fa-icon "truck" fixedWidth=true}} Docks
        {{/nav-tab}}
        {{#nav-tab "warehouse.admin.gates.index"}}
          {{fa-icon "flag" fixedWidth=true}} Entrance / Exits
        {{/nav-tab}}
        {{#nav-tab "warehouse.admin.roads.index"}}
          {{fa-icon "road" fixedWidth=true}} Roads
        {{/nav-tab}}
        {{#nav-tab "warehouse.admin.scales.index"}}
          {{fa-icon "balance-scale" fixedWidth=true}} Weight Stations
        {{/nav-tab}}
        {{#nav-tab "warehouse.admin.walls.index"}}
          {{fa-icon "industry" fixedWidth=true}} walls
        {{/nav-tab}}
      </ul>
    {{/yields.links}}
    {{outlet}}

    {{#yields.footer}}
      {{modal-footer adminMode=true}}
    {{/yields.footer}}
  {{/modal-view}}
{{/modal-dialog}}

